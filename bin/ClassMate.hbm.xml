<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="JavaFiles.ClassMate" table="CLASSMATE">
		<id name="id" column="ID" type="integer">
			<generator class="assigned"></generator>
		</id>
		<property name="firstName" column="FIRST_NAME" type="string"></property>
		<property name="lastName" column="LAST_NAME" type="string"></property>
		<property name="hobby" column="HOBBY" type="string"></property>
		<property name="birthYear" column="BIRTH_YEAR" type="integer"></property>
		<property name="mobile" column="MOBILE" type="string"></property>
	</class>
</hibernate-mapping>